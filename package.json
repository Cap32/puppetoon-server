{
  "name": "puppetoon-server",
  "description": "puppetoon-server",
  "version": "0.0.0-2",
  "main": "lib/index",
  "bin": {
    "puppetoon-server": "bin/puppetoon",
    "puppetoon": "bin/puppetoon"
  },
  "files": [
    "*.md",
    "lib"
  ],
  "scripts": {
    "start": "yarn cli start",
    "start:prod": "yarn build && ./bin/puppetoon.js start",
    "cli": "cross-env NODE_ENV=development ./bin/puppetoon.js",
    "client": "babel-node test/client.js",
    "prebuild": "rimraf lib",
    "build": "babel src -d lib",
    "preversion": "yarn build"
  },
  "author": "Cap32",
  "repository": "Cap32/puppetoon-server",
  "engines": {
    "node": ">=4.2.1"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "fs-extra": "^4.0.2",
    "home-or-tmp": "^3.0.0",
    "lodash": "^4.17.4",
    "p-queue": "^2.2.0",
    "pot-js": "^0.0.0-beta.41",
    "pot-logger": "^0.2.1",
    "puppeteer": "^0.11.0",
    "signal-exit": "^3.0.2",
    "uuid": "^3.1.0",
    "ws": "^3.2.0",
    "yargs": "^9.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-es2015-destructuring": "^6.19.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-es2015-parameters": "^6.18.0",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "cross-env": "^4.0.0",
    "delay": "^2.0.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^20.0.4",
    "rimraf": "^2.5.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "modulePathIgnorePatterns": [
      "node_modules",
      "lib"
    ]
  }
}
